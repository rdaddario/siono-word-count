{
  "name": "word-count",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "npm run test:lint && npm run test:coverage",
    "build": "npm run tsc --project tsconfig.compile.json",
    "build:w": "tsc --project tsconfig.compile.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:unit": "cross-env NODE_ENV=test jest --coverage=false",
    "test:coverage": "npm run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "@tsed/ajv": "^6.98.5",
    "@tsed/common": "^6.98.5",
    "@tsed/core": "^6.98.5",
    "@tsed/di": "^6.98.5",
    "@tsed/exceptions": "^6.98.5",
    "@tsed/json-mapper": "^6.98.5",
    "@tsed/platform-express": "^6.98.5",
    "@tsed/schema": "^6.98.5",
    "ajv": "^8.9.0",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.17.0",
    "@tsed/cli-plugin-jest": "3.17.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/method-override": "0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "main": "index.js",
  "keywords": [],
